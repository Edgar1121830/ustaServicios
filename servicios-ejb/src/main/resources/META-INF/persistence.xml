		<class>co.com.servicios.modelo.ReportetareaPK</class>
		<class>co.com.servicios.modelo.Respuestapreta</class>
		<class>co.com.servicios.modelo.RespuestapretaPK</class>
		<class>co.com.servicios.modelo.Rol</class>
