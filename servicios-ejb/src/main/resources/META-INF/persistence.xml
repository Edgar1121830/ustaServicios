		<class>co.com.servicios.modelo.Opcionespreguntaseva</class>
		<class>co.com.servicios.modelo.Opcionespreguntastarea</class>
		<class>co.com.servicios.modelo.Persona</class>
		<class>co.com.servicios.modelo.Preguntaseva</class>
		<class>co.com.servicios.modelo.Preguntastarea</class>
