		<class>co.com.servicios.modelo.Evaluacion</class>
		<class>co.com.servicios.modelo.Fas</class>
		<class>co.com.servicios.modelo.Habilidade</class>
		<class>co.com.servicios.modelo.Ingreso</class>
		<class>co.com.servicios.modelo.Inscripcion</class>
		<class>co.com.servicios.modelo.Institucione</class>
		<class>co.com.servicios.modelo.Instruccione</class>
		<class>co.com.servicios.modelo.Nivele</class>
